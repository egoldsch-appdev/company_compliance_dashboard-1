<div class="page-header">
  <h1>
    Employees
    <a href="/employees/new" class="btn btn-lg btn-success">New Employee</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Mcm userid</th>
        <th>First name</th>
        <th>Last name</th>
        <th>Doh</th>
        <th>Branch</th>
        <th>Dept start date</th>
        <th>Prev dept</th>
        <th>Prev dept start date</th>
        <th>Department</th>
        <th>Actions</th>
      </tr>

      <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.mcm_userid %></td>
        <td><%= employee.first_name %></td>
        <td><%= employee.last_name %></td>
        <td><%= employee.doh %></td>
        <td><%= employee.branch %></td>
        <td><%= employee.dept_start_date %></td>
        <td><%= employee.prev_dept_id %></td>
        <td><%= employee.prev_dept_start_date %></td>
        <td><%= employee.department_id %></td>
        <td>
          <a href="/employees/<%= employee.id %>" class="btn btn-primary">Show</a>
          <a href="/employees/<%= employee.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_employee/<%= employee.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
