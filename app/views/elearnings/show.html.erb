<div class="page-header">
  <h3>Elearning #<%= @elearning.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Title</dt>
      <dd><%= @elearning.title %></dd>

      <dt>Duration</dt>
      <dd><%= @elearning.duration %></dd>

      <dt>Link</dt>
      <dd><%= @elearning.link %></dd>

      <dt>Due date</dt>
      <dd><%= @elearning.due_date %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/elearnings" class="btn btn-primary">
        Back
      </a>
      <a href="/elearnings/<%= @elearning.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_elearning/<%= @elearning.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
